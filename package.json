{
  "name": "trigger-dnd",
  "productName": "Trigger DnD",
  "version": "1.0.9",
  "description": "Trigger DnD modes all over the place",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "petergoes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voorhoede-labs/trigger-dnd.git"
  },
  "bugs": {
    "url": "https://github.com/voorhoede-labs/trigger-dnd/issues"
  },
  "homepage": "https://github.com/voorhoede-labs/trigger-dnd#readme",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "src/assets/icon.icns",
        "osxSign": true
      },
      "electronWinstallerConfig": {
        "name": "trigger_dnd"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "voorhoede-labs",
        "name": "trigger-dnd",
        "options": {
          "protocol": "https",
          "host": "api.github.com"
        }
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "triggerdnd"
      },
      "darwinDarkModeSupport": true
    }
  },
  "dependencies": {
    "@sindresorhus/do-not-disturb": "0.2.0",
    "electron-compile": "6.4.4",
    "electron-devtools-installer": "2.2.4",
    "electron-log": "2.2.17",
    "electron-squirrel-startup": "1.0.0",
    "eslint": "5.12.0",
    "moment": "2.23.0",
    "node-luxafor": "2.1.0",
    "slack": "11.0.1",
    "update-electron-app": "1.3.0",
    "vue": "2.5.21",
    "vuetify": "1.4.0"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "electron-forge": "5.2.3",
    "electron-prebuilt-compile": "4.0.0"
  }
}
