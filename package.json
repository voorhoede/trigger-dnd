{
  "name": "trigger-dnd",
  "productName": "Trigger DnD",
  "version": "1.0.17",
  "description": "Trigger DnD modes all over the place",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "beforepublish": "rm -rf ./node_modules/.cache && rm -rf ./out && xattr -c . && xattr -cr ./src && xattr -d com.apple.quarantine . && xattr -dr com.apple.quarantine ./src",
    "publish": "electron-forge publish",
    "prettier-format": "prettier --config .prettierrc '**/*.{js,vue}' --write"
  },
  "keywords": [],
  "author": "petergoes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voorhoede-labs/trigger-dnd.git"
  },
  "bugs": {
    "url": "https://github.com/voorhoede-labs/trigger-dnd/issues"
  },
  "homepage": "https://github.com/voorhoede-labs/trigger-dnd#readme",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "icon": "src/assets/icon.icns",
        "osxSign": true
      },
      "electronWinstallerConfig": {
        "name": "trigger_dnd"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "voorhoede-labs",
        "name": "trigger-dnd",
        "options": {
          "protocol": "https",
          "host": "api.github.com"
        }
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "triggerdnd"
      },
      "darwinDarkModeSupport": true
    }
  },
  "dependencies": {
    "@sindresorhus/do-not-disturb": "^1.1.0",
    "auto-launch": "5.0.5",
    "dotenv": "8.2.0",
    "electron-compile": "6.4.4",
    "electron-devtools-installer": "3.1.1",
    "electron-log": "4.3.2",
    "electron-squirrel-startup": "1.0.0",
    "emoji-tree": "1.1.2",
    "googleapis": "67.1.0",
    "moment": "2.29.2",
    "node-luxafor": "2.1.0",
    "slack": "11.0.2",
    "update-electron-app": "2.0.1",
    "vue": "2.6.12",
    "vueify": "^9.4.1",
    "vuetify": "2.4.4"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "electron-forge": "5.2.4",
    "electron-prebuilt-compile": "8.2.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-vue": "^7.6.0",
    "prettier": "^2.2.1"
  }
}
